<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="false"
    android:background="@drawable/bg7">
    <include layout="@layout/top"/>

    <SearchView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/storyConstraintlayout"
        android:layout_width="match_parent"
        android:layout_height="581dp"
        android:clickable="false"
        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="@+id/shenhualinearLayout"
        app:layout_constraintEnd_toEndOf="@+id/minjianlinearLayout"
        app:layout_constraintStart_toEndOf="@+id/minjianlinearLayout"
        app:layout_constraintTop_toBottomOf="@+id/shenhualinearLayout">

        <LinearLayout
            android:id="@+id/minjianlinearLayout"
            android:clickable="false"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toTopOf="@+id/dangshilinearLayout"
            app:layout_constraintEnd_toStartOf="@+id/shenhualinearLayout"
            app:layout_constraintHeight_percent="0.5"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_weight="0.5"
            app:layout_constraintWidth_percent="0.5">

            <TextView
                android:id="@+id/minjiantextView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.125"
                android:background="@drawable/txvborder"
                android:text="TextView"
                android:textSize="18dp" />

            <TextView
                android:id="@+id/minjiantextView2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.125"
                android:background="@drawable/txvborder"
                android:textSize="18dp"
                android:text="民间1" />

            <TextView
                android:id="@+id/minjiantextView4"
                android:layout_weight="0.125"
                android:layout_width="match_parent"
                android:textSize="18dp"
                android:background="@drawable/txvborder"
                android:layout_height="match_parent"
                android:text="民间1" />

            <TextView
                android:id="@+id/minjiantextView5"
                android:background="@drawable/txvborder"
                android:layout_weight="0.125"
                android:textSize="18dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="民间1" />

            <TextView
                android:id="@+id/minjiantextView6"
                android:background="@drawable/txvborder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="18dp"
                android:layout_weight="0.125"
                android:text="民间1" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/shenhualinearLayout"
            android:clickable="false"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginRight="0dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="@+id/minjianlinearLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.5"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintStart_toEndOf="@+id/minjianlinearLayout"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_weight="0.5"
            app:layout_constraintWidth_percent="0.5">
            <TextView
                android:id="@+id/shenhuatextView"
                android:background="@drawable/txvborder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="18dp"
                android:layout_weight="0.125"
                android:text="TextView" />

            <TextView
                android:id="@+id/shenhuatextView2"
                android:background="@drawable/txvborder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="18dp"
                android:layout_weight="0.125"
                android:text="TextView" />

            <TextView
                android:id="@+id/shenhuatextView4"
                android:layout_weight="0.125"
                android:background="@drawable/txvborder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="18dp"
                android:text="TextView" />

            <TextView
                android:id="@+id/shenhuatextView5"
                android:layout_width="match_parent"
                android:background="@drawable/txvborder"
                android:layout_height="match_parent"
                android:layout_weight="0.125"
                android:text="TextView"
                android:textSize="18dp" />

            <TextView
                android:id="@+id/shenhuatextView6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/txvborder"
                android:textSize="18dp"
                android:layout_weight="0.125"
                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_height="0dp"
            android:layout_width="0dp"
            android:clickable="false"
            android:id="@+id/dangshilinearLayout"
            android:layout_marginLeft="0dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/wenhualinearLayout"
            app:layout_constraintHeight_percent="0.5"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/minjianlinearLayout"
            app:layout_constraintVertical_weight="0.5"
            app:layout_constraintWidth_percent="0.5">
            <TextView
                android:id="@+id/dangshitextView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.125"
                android:background="@drawable/txvborder"
                android:clickable="true"
                android:textSize="18dp"
                android:text="TextView" />

            <TextView
                android:id="@+id/dangshitextView2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.125"
                android:textSize="18dp"
                android:background="@drawable/txvborder"
                android:text="TextView" />

            <TextView
                android:id="@+id/dangshitextView4"
                android:layout_weight="0.125"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="18dp"
                android:background="@drawable/txvborder"
                android:text="TextView" />

            <TextView
                android:id="@+id/dangshitextView5"
                android:layout_weight="0.125"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/txvborder"
                android:textSize="18dp"
                android:text="TextView" />

            <TextView
                android:id="@+id/dangshitextView6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.125"
                android:textSize="18dp"
                android:background="@drawable/txvborder"
                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_height="0dp"
            android:layout_width="0dp"
            android:id="@+id/wenhualinearLayout"
            android:clickable="false"
            android:layout_marginRight="0dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.5"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintStart_toEndOf="@+id/dangshilinearLayout"
            app:layout_constraintTop_toBottomOf="@+id/minjianlinearLayout"
            app:layout_constraintVertical_weight="0.5"
            app:layout_constraintWidth_percent="0.5">
            <TextView
                android:id="@+id/wenhuatextView"
                android:background="@drawable/txvborder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.125"
                android:textSize="18dp"
                android:text="TextView" />

            <TextView
                android:id="@+id/wenhuatextView2"
                android:layout_width="match_parent"
                android:background="@drawable/txvborder"
                android:layout_height="match_parent"
                android:textSize="18dp"
                android:layout_weight="0.125"
                android:text="TextView" />

            <TextView
                android:id="@+id/wenhuatextView4"
                android:layout_weight="0.125"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/txvborder"
                android:textSize="18dp"
                android:text="TextView" />

            <TextView
                android:id="@+id/wenhuatextView5"
                android:layout_weight="0.125"
                android:background="@drawable/txvborder"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="18dp"
                android:text="TextView" />

            <TextView
                android:id="@+id/wenhuatextView6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="18dp"
                android:background="@drawable/txvborder"
                android:layout_weight="0.125"
                android:text="TextView" />

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <include
        layout="@layout/bottom"
        android:layout_width="match_parent"
        android:layout_height="67dp" />

</LinearLayout>