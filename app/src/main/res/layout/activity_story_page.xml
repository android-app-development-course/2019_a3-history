        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="@+id/shenhualinearLayout"
        app:layout_constraintEnd_toEndOf="@+id/minjianlinearLayout"
        app:layout_constraintStart_toEndOf="@+id/minjianlinearLayout"
        app:layout_constraintTop_toBottomOf="@+id/shenhualinearLayout">

